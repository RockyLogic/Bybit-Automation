{
  "name": "bot-template",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Rocky#0001",
  "license": "MIT",
  "bin": "./dist/src/index.js",
  "pkg": {
    "assets": "./src/static/*",
    "targets": [
      "node14-win-x64",
      "node14-macos-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -b && ts-node ./build.ts && pkg --output dist/Bot-Template ."
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/prompts": "^2.0.14"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "glob": "^7.2.0",
    "javascript-obfuscator": "^3.2.2",
    "node-bash-title": "^0.0.2",
    "pkg": "^5.5.2",
    "prompts": "^2.4.2",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
